class FindAllOccurrences {
  public static int[] findAllOccurance(int[] arr, int target) {
          ArrayList<Integer> returnList = new ArrayList<>();
          for (int i = 0; i < arr.length; i++) {
              if (arr[i] == target) {
                  returnList.add(i);
              }
          }
          int [] newArr = new int[returnList.size()];
          for (int i = 0; i < newArr.length; i++) {
              newArr[i] = returnList.get(i);
          }
          //int newArr[] = returnList.toArray();
          return newArr;
  }
}
